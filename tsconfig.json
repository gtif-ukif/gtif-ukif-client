{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": ".",
    "target": "esnext",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "Node"
  },
  "exclude": ["node_modules", ".eodash", "public", "dist"],
  "include": ["**/*.js", "**/*.vue", "**/*.d.ts", "**/*.ts"]
}
